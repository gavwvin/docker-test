@model IEnumerable<Docker_Test.Product>
@{
    ViewBag.Title = "Home Page";
}
<h1>Test</h1>
<main>
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-md-4">
                <h2>@item.Name</h2>
                <p>@item.Price</p>
            </div>
        }
    </div>
</main>